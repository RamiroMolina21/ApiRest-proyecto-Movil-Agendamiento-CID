  "components": {
    "schemas": {
      "AgregarEstudiantesTutoriaDto": {
        "required": [
          "estudianteIds"
        ],
        "type": "object",
        "properties": {
          "estudianteIds": {
            "minItems": 1,
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            }
          }
        },
        "additionalProperties": false
      },
      "HorarioCreateDto": {
        "required": [
          "cupos",
          "espacio",
          "estado",
          "fechaFin",
          "fechaInicio",
          "horaFin",
          "horaInicio",
          "titulo",
          "usuarioId"
        ],
        "type": "object",
        "properties": {
          "titulo": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "fechaInicio": {
            "type": "string",
            "format": "date-time"
          },
          "fechaFin": {
            "type": "string",
            "format": "date-time"
          },
          "horaInicio": {
            "type": "string",
            "format": "date-time"
          },
          "horaFin": {
            "type": "string",
            "format": "date-time"
          },
          "cupos": {
            "maxLength": 60,
            "minLength": 0,
            "type": "string"
          },
          "espacio": {
            "maxLength": 60,
            "minLength": 0,
            "type": "string"
          },
          "estado": {
            "maxLength": 60,
            "minLength": 0,
            "type": "string"
          },
          "usuarioId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "required": [
          "contrasena",
          "correo"
        ],
        "type": "object",
        "properties": {
          "correo": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "contrasena": {
            "minLength": 6,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RetroalimentacionCreateDto": {
        "required": [
          "calificacion",
          "comentario",
          "tutoriaId",
          "usuarioId"
        ],
        "type": "object",
        "properties": {
          "comentario": {
            "maxLength": 300,
            "minLength": 0,
            "type": "string"
          },
          "calificacion": {
            "maximum": 5,
            "minimum": 1,
            "type": "number",
